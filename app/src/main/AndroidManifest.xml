<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moi.museo3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PantallaEntrada">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MantenedorUsuario"
            android:parentActivityName=".PantallaEntrada" />
        <activity
            android:name=".MantenedorMuseos"
            android:parentActivityName=".PantallaEntrada" />
        <activity
            android:name=".Exposiciones"
            android:parentActivityName=".PantallaEntrada" />
        <activity
            android:name=".Reportes"
            android:parentActivityName=".PantallaEntrada" />
        <activity android:name=".ListarMuseos" />
        <activity android:name=".ListarUsuarios" />
        <activity android:name=".Exposicion" />
        <activity android:name=".ListarExposiciones"></activity>
    </application>

</manifest>